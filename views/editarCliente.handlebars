<link rel="stylesheet" href="/public/css/editarCliente.css"
<link rel="stylesheet" href="/public/media/media.css">
<form action="/adm/clientes/editar" method="post">
  <div class="form-group">
    <label for="id">ID</label>
    <input type="text" class="form-control" placeholder="" id="id" name="id" value="{{cliente.dataValues.id}}" readonly>
  </div>
  <div class="form-group">
    <label for="nome">Nome</label>
    <input type="text" class="form-control" placeholder="Informe o nome do cliente" id="nome" name="nome" value="{{cliente.dataValues.nome}}">
  </div>
  <div class="form-group">
    <label for="cpf">CPF</label>
    <input type="text" class="form-control" placeholder="Informe o CPF do cliente" id="cpf" name="cpf" value="{{cliente.dataValues.cpf}}">
  </div>
  <div class="form-group">
    <label for="email">ID do Funcion√°rio</label>
    <input type="text" class="form-control" placeholder="Informe o e-mail do cliente" id="email" name="email" value="{{cliente.dataValues.email}}">
  </div>
  <button type="submit" class="btn btn-primary">Enviar</button>
</form>